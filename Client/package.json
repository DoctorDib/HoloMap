{
    "author": "James Dibnah",
    "name": "holomap",
    "description": "Holomap",
    "version": "0.0.1",
    "private": true,
    "main": "index.js",
    "homepage": "./",
    "scripts": {
        "start": "concurrently -k \"npm run client:start\" \"npm run electron:start\"",
        
        "client:start": "set PORT=8080 && react-scripts start",
        "electron:start": "wait-on tcp:127.0.0.1:8080 && electron .",

        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "build": "react-scripts build",
        "prod:start": "serve -s ./build -l 8080",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "format": "npm run format:eslint && npm run format:prettier",
        "format:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
        "format:prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "autoprefixer": "10.4.5"
    },
    "overrides": {
        "autoprefixer": "10.4.5"
    },
    "dependencies": {
        "@edorivai/react-smooth-dnd": "^0.11.2",
        "@electron/remote": "^2.0.10",
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.1.2",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@react-spring/web": "^9.5.4",
        "@tldraw/tldraw": "^1.24.5",
        "@use-gesture/react": "^10.2.20",
        "axios": "^0.27.2",
        "classnames": "^2.3.1",
        "concurrently": "^8.2.2",
        "crypto-js": "^4.1.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-iframe": "^1.8.5",
        "react-lineto": "^3.3.0",
        "react-projection-mapping": "^1.98.1",
        "react-redux": "^7.2.9",
        "react-router-dom": "^6.3.0",
        "react-scripts": "^5.0.1",
        "react-toastify": "^9.0.7",
        "react-use-cookie": "^1.4.0",
        "redux": "^4.2.0",
        "redux-thunk": "^2.4.2",
        "serve": "^14.2.0",
        "socket.io-client": "^4.7.5",
        "spinners-react": "^1.0.7",
        "use-sound": "^4.0.1",
        "usehooks-ts": "^2.9.1",
        "wait-on": "^7.2.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.4.0",
        "@electron-forge/maker-squirrel": "^7.4.0",
        "@types/crypto-js": "^4.1.1",
        "@types/electron": "^1.6.10",
        "@types/react": "^18.0.15",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "electron": "^25.9.8",
        "electron-squirrel-startup": "^1.0.1",
        "eslint": "^8.2.0",
        "eslint-config-airbnb-typescript": "^15.0.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-react": "^7.25.3",
        "eslint-plugin-react-hooks": "^4.3.0",
        "fs": "^0.0.1-security",
        "prettier": "^2.7.1",
        "typescript": "^4.7.4",
        "webpack": "^5.93.0"
    }
}
