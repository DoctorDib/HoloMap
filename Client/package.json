{
    "author": "James Dibnah",
    "name": "holomap",
    "description": "Holomap",
    "version": "0.0.1",
    "resolution": {
        "width": 640,
        "height": 480
    },
    "private": true,
    "main": "index.js",
    "homepage": "./",
    "scripts": {
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "build": "react-scripts build",
        "prod:start": "serve -s ./build -l 8080",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "format": "npm run format:eslint && npm run format:prettier",
        "format:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
        "format:prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
        
        "client:start": "cross-env PORT=8080 react-scripts start",
        "electron:start": "wait-on tcp:127.0.0.1:8080 && electron .",
        "start": "cross-env PORT=8080 NODE_ENV=development npm run client:start",
        "start:prod": "cross-env NODE_ENV=production npm run build && npm run electron:start"
    
    
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "autoprefixer": "10.4.5"
    },
    "overrides": {
        "autoprefixer": "10.4.5"
    },
    "dependencies": {
        "@edorivai/react-smooth-dnd": "^0.11.2",
        "@electron/remote": "^2.1.2",
        "@fortawesome/fontawesome-svg-core": "^6.6.0",
        "@fortawesome/free-solid-svg-icons": "^6.6.0",
        "@fortawesome/react-fontawesome": "^0.2.2",
        "@react-spring/web": "^9.7.5",
        "@tldraw/tldraw": "^3.3.0",
        "@use-gesture/react": "^10.3.1",
        "axios": "^1.7.7",
        "classnames": "^2.5.1",
        "concurrently": "^9.0.1",
        "crypto-js": "^4.2.0",
        "dotenv-webpack": "^8.1.0",
        "os": "^0.1.2",
        "path": "^0.12.7",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-iframe": "^1.8.5",
        "react-lineto": "^3.3.0",
        "react-projection-mapping": "^1.98.1",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.26.2",
        "react-scripts": "^5.0.1",
        "react-toastify": "^10.0.5",
        "react-use-cookie": "^1.6.1",
        "redux": "^5.0.1",
        "redux-thunk": "^3.1.0",
        "serve": "^14.2.3",
        "socket.io-client": "^4.8.0",
        "spinners-react": "^1.0.7",
        "use-sound": "^4.0.3",
        "usehooks-ts": "^3.1.0",
        "wait-on": "^8.0.1"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.5.0",
        "@electron-forge/maker-squirrel": "^7.5.0",
        "@types/crypto-js": "^4.2.2",
        "@types/electron": "^1.6.10",
        "@types/react": "^18.0.15",
        "cross-env": "^7.0.3",
        "crypto-browserify": "^3.12.0",
        "electron": "^32.2.0",
        "electron-squirrel-startup": "^1.0.1",
        "eslint": "^8.39.0",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-react": "^7.37.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "fs": "^0.0.2",
        "path-browserify": "^1.0.1",
        "prettier": "^3.3.3",
        "process": "^0.11.10",
        "sass": "^1.79.4",
        "source-map-loader": "^5.0.0",
        "ts-loader": "^9.5.1",
        "typescript": "^4.9.5",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4"
    }
}
